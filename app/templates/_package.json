{
  "name": "<%= packageName %>",
  "version": "0.0.0",
  "private": true,
  "description": "<%= description %>",
  "keywords": [
    "<%= packageName %>",
    "lit-element",
    "lit-html",
    "polymer",
    "typescript",
    "web-components"
  ],
  "homepage": "<%= homepage %>",
  "repository": {
    "type": "git",
    "url": "<%= repoUrl %>"
  },
  "license": "MIT",
  "author": {
    "name": "<%= authorName %>",
    "email": "<%= authorEmail %>",
    "url": "<%= authorUrl %>"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/my-element.js",
  "module": "./dist/my-element.js",
  "typings": "./dist/my-element.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.test.json",
    "clean": "rm -rf ./*.*js ./*.d.ts dist/",
    "lint": "tslint --project tsconfig.json --config tslint.prod.json --format stylish",
    "lint:debug": "tslint --project tsconfig.json --config tslint.json --format stylish",
    "prepublishOnly": "rm -rf dist/ && rollup -c ./rollup.config.js",
    "rollup": "rollup -c ./rollup.config.js",
    "test": "npm run lint && npm run build && wct"
  },
  "dependencies": {
    "lit-element": "^2.1.0",
    "lit-html": "^1.0.0"
  },
  "devDependencies": {
    "@messageflow/tslint-config": "^1.3.0",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "rollup": "^1.7.0",
    "rollup-plugin-filesize": "^6.0.1",
    "rollup-plugin-minify-html-literals": "^1.2.2",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-tslint": "^0.2.2",
    "rollup-plugin-typescript2": "^0.20.1",
    "@webcomponents/webcomponentsjs": "^2.2.7",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "tslint": "^5.14.0",
    "typescript": "^3.3.4000",
    "wct-mocha": "^1.0.0"
  },
  "engines": {
    "node": ">= 10.15.1",
    "npm": ">= 6.4.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
